{
  "name": "react-api-designer",
  "version": "0.1.0",
  "description": "A web editor for creating and sharing RAML API specifications",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mulesoft/react-api-designer.git"
  },
  "main": "./dist/main.js",
  "scripts": {
    "start": "webpack -d --config ./webpack/standalone/config.worker.js && npm run concat && react-scripts start",
    "build": "webpack --config ./webpack/standalone/config.worker.js && npm run concat && react-scripts build",
    "watch-worker": "webpack -d --watch --config ./webpack/standalone/config.worker.js",
    "component": "npm run component-worker && npm run component-main && npm run concat",
    "component-worker": "webpack --config ./webpack/component/config.worker.js",
    "component-main": "webpack --config ./webpack/component/config.js",
    "concat": "node ./webpack/console/concat.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.0.1",
    "flow-bin": "^0.37.4",
    "react-scripts": "^0.8.5",
    "webpack": "^1.14.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "json-loader": "^0.5.4",
    "css-loader": "^0.26.1",
    "ignore-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  },
  "dependencies": {
    "@mulesoft/anypoint-components": "^2.42.5",
    "@mulesoft/anypoint-icons": "^4.2.0",
    "@mulesoft/anypoint-styles": "^1.1.0",
    "api-console": "^3.0.13",
    "browser-request": "^0.3.3",
    "classnames": "^2.2.5",
    "concat-files": "^0.1.1",
    "immutable": "^3.8.1",
    "jszip": "^3.1.3",
    "monaco-editor": "^0.8.1",
    "oas-raml-converter": "0.2.0",
    "raml-1-parser": "1.1.14",
    "raml-suggestions": "0.0.27",
    "react": "^15.4.2",
    "react-async-script-loader": "^0.2.2",
    "react-dom": "^15.4.2",
    "react-file-download": "^0.3.2",
    "react-json-tree": "^0.10.1",
    "react-markdown": "^2.4.4",
    "react-monaco-editor": "^0.7.3",
    "react-redux": "^4.4.5",
    "react-split-pane": "^0.1.57",
    "react-toggle-button": "^2.1.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
  }
}
